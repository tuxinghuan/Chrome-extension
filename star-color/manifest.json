{
    "manifest_version": 2,
    "name" : "change contribution color",
    "version": "1.0",
    "description": "change github contribution color",
    "icons":{
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action":{
        "default_icon":"images/icon48.png",
        "default_title":"change contribution color" 
    },
    "content_scripts": [
        {
          "matches": ["*://github.com/*"],
          "js": ["js/changeContributionColor.js"]
        }
    ]
}